{
  "database": {
    "url": "postgresql://username:password@localhost:5432/weewee_def",
    "neonUrl": "postgresql://username:password@ep-xxx-xxx-xxx.region.aws.neon.tech/neondb"
  },
  "api": {
    "baseUrl": "http://localhost:3000/api",
    "key": "your_api_key_here"
