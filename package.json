    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
